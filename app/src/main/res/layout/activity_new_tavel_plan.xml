    <EditText
        android:id="@+id/tv_PlanName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/view"
        android:layout_marginBottom="10dp"
        android:layout_marginTop="53dp"
        android:hint="@string/hint_plan_name"
        android:paddingLeft="10dp"
        android:singleLine="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />


    <EditText
        android:id="@+id/et_PlaceName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/tv_PlanName"
        android:layout_marginBottom="10dp"
        android:hint="@string/hint_destination"
        android:paddingLeft="10dp"
        android:singleLine="true"
        android:layout_alignRight="@+id/tv_PlanName"
        android:layout_alignEnd="@+id/tv_PlanName" />


    <Spinner
        android:id="@+id/sp_Months"
        android:layout_width="75dp"
        android:layout_height="35dp"
        android:drawablePadding="10dp"

        android:layout_below="@+id/include"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_toStartOf="@+id/tv_NewPlan"
        android:layout_alignRight="@+id/include"
        android:layout_alignEnd="@+id/include" />


    <include
        android:id="@+id/include"
        layout="@layout/seek_bar"
        android:layout_width="170dp"
        android:layout_height="35dp"
        android:drawableLeft="@drawable/location_icon"
        android:drawablePadding="11dp"
        android:hint="@string/hint_destination"
        android:layout_below="@+id/et_PlaceName"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />


    //TextViews for SeekBarlegend
    //TextView to display result

    <TextView
        android:id="@+id/tvResult"
        android:layout_width="75dp"
        android:layout_height="35dp"
        android:gravity="center"
        android:paddingBottom="2dp"
        android:paddingLeft="2dp"
        android:paddingRight="2dp"
        android:paddingTop="2dp"
        android:text="@string/label_travel_days"
        android:textAlignment="center"
        android:textColor="@color/colorBlack"
        android:textSize="12sp"
        android:layout_below="@+id/et_PlaceName"
        android:layout_toRightOf="@+id/include"
        android:layout_toEndOf="@+id/include" />


